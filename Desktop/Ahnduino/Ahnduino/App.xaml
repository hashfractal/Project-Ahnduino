<Application x:Class="Ahnduino.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Ahnduino"
             StartupUri="Wins/Auth.xaml">
	<Application.Resources>
		<ResourceDictionary>
			<FontFamily x:Key="Default">/Fonts/#배달의민족 한나체 Air</FontFamily>

			<ResourceDictionary.MergedDictionaries>
				<ResourceDictionary Source="Themes/MenuButton.xaml"/>
				<ResourceDictionary Source="Themes/AuthTextbox.xaml"/>
				<ResourceDictionary Source="Themes/ContactCard.xaml"/>
				<ResourceDictionary Source="Themes/ChatItem.xaml"/>
				<ResourceDictionary Source="Themes/MessageBox.xaml"/>
			</ResourceDictionary.MergedDictionaries>
		</ResourceDictionary>
	</Application.Resources>
</Application>
