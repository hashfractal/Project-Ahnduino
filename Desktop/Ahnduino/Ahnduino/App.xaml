<Application x:Class="Ahnduino.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Ahnduino"
             StartupUri="Wins/MainWindow.xaml">
	<Application.Resources>
		<ResourceDictionary>
			<FontFamily x:Key="OptimusPrinceps">/Fonts/#OptimusPrinceps</FontFamily>
			<FontFamily x:Key="OptimusPrincepsSB">/Fonts/#OptimusPrincepsSemiBold</FontFamily>
			<FontFamily x:Key="NanumMyeongjo">/Fonts/#NanumMyeongjo</FontFamily>
			
			<ResourceDictionary.MergedDictionaries>
				<ResourceDictionary Source="Themes/AuthButton.xaml"/>
				<ResourceDictionary Source="Themes/AuthTextbox.xaml"/>
				<ResourceDictionary Source="Themes/ContactCard.xaml"/>
				<ResourceDictionary Source="Themes/ChatItem.xaml"/>
				<ResourceDictionary Source="Themes/MessageBox.xaml"/>
			</ResourceDictionary.MergedDictionaries>
		</ResourceDictionary>
	</Application.Resources>
</Application>
